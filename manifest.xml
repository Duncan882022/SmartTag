<?xml version="1.0" encoding="UTF-8"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:type="TaskPaneApp">

  <Id>3b96f2a8-5b7d-4df8-99a5-8bba3faaa3c1</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Just Engineer Tech</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="SmartTag Copilot"/>
  <Description DefaultValue="Add-in tìm kiếm và chèn tag từ SharePoint list TagLibrary." />

  <IconUrl DefaultValue="https://duncan882022.github.io/SmartTag/icon32.png" />
  <HighResolutionIconUrl DefaultValue="https://duncan882022.github.io/SmartTag/icon80.png" />

  <SupportUrl DefaultValue="https://justengineertech.sharepoint.com/sites/E-Office" />

  <AppDomains>
    <AppDomain>https://duncan882022.github.io</AppDomain>
    <AppDomain>https://justengineertech.sharepoint.com</AppDomain>
  </AppDomains>

  <Hosts>
    <Host Name="Document"/>
  </Hosts>

  <DefaultSettings>
    <SourceLocation DefaultValue="https://duncan882022.github.io/SmartTag/SmartTagCopilot.html"/>
  </DefaultSettings>

  <Permissions>ReadWriteDocument</Permissions>

  <VersionOverrides xmlns="http://schemas.microsoft.com/office/mailappversionoverrides/1.0" xsi:type="VersionOverridesV1_0">
    <Hosts>
      <Host xsi:type="Document">
        <DesktopFormFactor>
          <GetStarted>
            <Title resid="residTitle"/>
            <Description resid="residDesc"/>
            <LearnMoreUrl resid="residUrl"/>
          </GetStarted>
          <FunctionFile resid="residFunctionFile"/>
          <ExtensionPoint xsi:type="TaskPane">
            <SourceLocation resid="residSourceLocation"/>
            <Title resid="residTitle"/>
            <SupportUrl resid="residUrl"/>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0">
        <bt:Image id="icon32" DefaultValue="https://duncan882022.github.io/SmartTag/icon32.png"/>
        <bt:Image id="icon80" DefaultValue="https://duncan882022.github.io/SmartTag/icon80.png"/>
      </bt:Images>
      <bt:Urls xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0">
        <bt:Url id="residUrl" DefaultValue="https://justengineertech.sharepoint.com/sites/E-Office"/>
        <bt:Url id="residSourceLocation" DefaultValue="https://duncan882022.github.io/SmartTag/SmartTagCopilot.html"/>
        <bt:Url id="residFunctionFile" DefaultValue="https://duncan882022.github.io/SmartTag/SmartTagCopilot.html"/>
      </bt:Urls>
      <bt:ShortStrings xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0">
        <bt:String id="residTitle" DefaultValue="SmartTag Copilot"/>
      </bt:ShortStrings>
      <bt:LongStrings xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0">
        <bt:String id="residDesc" DefaultValue="Add-in chèn tag từ SharePoint list vào Word."/>
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>
